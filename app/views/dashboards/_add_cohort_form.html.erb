<!-- Feedback Block -->
         <section id="feedback-block" class="e-block-regular text-center bg-color01" style="padding-top: 50px">
           <h3 class="font-accident-two-normal e-uppercase text-center" id='add-cohort'>Add Cohort</h3>
           <div class="add-cohort">
            <div class="container">

                  <div class="e-divider-1"></div>
                  <p class="small fontcolor-medium text-center">
                      Add your Cohort's CSV roster so they can add projects to the gallery <br> We'll take care of the rest :)
                  </p>
                  <p id="panel-update">

                  </p>
               <div class="e-divider-4"></div>
               <div class="row">
                  <div class="col-md-2">&nbsp;</div>
                  <div class="col-md-8 e-centered wow fadeInDown" data-wow-delay="0.6s" data-wow-offset="10">
                    <div id="form-messages">
                     <div class="e-divider-2"></div>
                       <%= form_for Cohort.new, :builder => CohortCSVFormBuilder, remote: true, class: "wpcf7-form" do |f| %>
                          <div class="field">
                             <%= f.text_field :name, placeholder: "name" %>
                          </div>
                          <div class="field">
                            <%= f.csv_upload_field %>
                          </div>
                          <div class="e-divider-1"></div>
                          <div class="e-divider-1"></div>
                          <!-- <div class="e-divider-1"></div> -->
                          <!-- <div class="e-divider-2"></div> -->

                          <div class="field text-center">
                            <%= f.submit class: "btn btn-lg btn-blk", data_attr: 'submit-btn' %>
                          <% end %>
                          </div>
                        </div>
                     </form>
                  </div>
                </div>
                  <div class="col-md-2">&nbsp;</div>
                  <div class="col-md-12 divider-dynamic"></div>
             </div>
            </div>
          </div>
            <!-- <div class="e-divider-4"></div> -->

         </section>
         <!-- /Feedback Block -->
