<div class="row project-show-row">
   <!-- Colleagues -->
   <section id="colleagues" class="e-block-short" style="padding-top: 0px; padding-bottom: 0px">
      <div class="container-fluid padding-side-50"  style="background: #F2F2F2; padding-top: 10px; padding-bottom: 10px">

         <div class="text-center">
            <h3 class="font-accident-two-normal e-uppercase">Project Collaborators</h3>
            <div class="e-divider-4"></div>
         </div>

         <div class="row">
          <% project.users.each do |user| %>
             <div class="col-md-<%= col_md(project.users.length) %> col-xs-<%= project.users.length %> text-center">
                <div class="e-width-80 e-centered collegaues">
                   <%= image_tag user.avatar.url(:medium), alt: "#{user.full_name}", class: "collab-img" %>
                   <div class="e-divider-3"></div>
                   <p class="font-accident-one-bold e-uppercase"><%= link_to user.full_name, user_path(user) %></p>
                </div>
             </div>
          <% end %>
         </div>
      </div>
   </section>
</div>
